{
  "name": "vet-sign",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:proximacapital/vet-sign.git"
  },
  "homepage": "https://github.com/proximacapital/vet-sign/blob/prod/README.md",
  "engines": {
    "node": "^16.6.1",
    "npm": ">=7.20.0"
  },
  "prepublish": "npm run build",
  "files": [
    "Dist"
  ],
  "bin": {
    "vet-sign": "./bin/vet-sign"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "compile": "gulp build",
    "build": "gulp build",
    "lint": "gulp lint",
    "test": "gulp test",
    "coverage": "gulp coverage"
  },
  "eslintConfig": {
    "extends": "@proxima-oss/eslint-config/style/standard"
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "c8": {
    "all": true,
    "include": [
      "Src"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "@vechain/connex-driver": "2.0.8",
    "@vechain/connex-framework": "2.0.8",
    "commander": "9.0.0",
    "lodash": "4.17.21",
    "thor-devkit": "2.0.4"
  },
  "devDependencies": {
    "@proxima-oss/eslint-config": "6.0.0",
    "@types/lodash": "4.14.179",
    "ava": "4.1.0",
    "c8": "7.11.0",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "markdownlint": "0.25.1",
    "markdownlint-cli": "0.31.1",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.13.0",
    "tslint": "6.1.3",
    "ttypescript": "1.5.13",
    "typescript": "4.9.4",
    "typescript-transform-paths": "3.3.1"
  },
  "volta": {
    "node": "16.6.1",
    "npm": "7.20.3"
  }
}
